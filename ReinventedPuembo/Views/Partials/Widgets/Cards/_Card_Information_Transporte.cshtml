<script>
    $(document).ready(function () {
        var pasa=0;
       
        $('.estudiante-button').click(function () {
       
                var estudianteId = $(this).data('estudiante-id');
                $('#idEst').val(estudianteId);
          
                cargarDetallesEstudiante(estudianteId);
           
            

        });


        function cargarDetallesEstudiante(estudianteId) {
            pasa=1;
            $.ajax({
                url: '/Home/ObtenerDetallesEstudiante/' + estudianteId,
                method: 'GET',
                success: function (data) {
                      pasa=0;
                    $('#nombresCompletos').val(data.nombres + ' ' + data.apellidos);
                    $('#nivelEscolar').val(data.nivelEscolar);
                    $('#callePrincipal').val(data.callePrincipal);
                    $('#calleSecundaria').val(data.calleSecundaria);
                    $('#numero').val(data.numero);
                    $('#idEst').val(estudianteId);
                    $('#RegistradoFlag').val(data.regist);

                    $('#AutorizaTerminosTranporte').prop('checked', false);
                    if(data.regist==true)
               {
                        $('#btnSolicitarServicio').prop('disabled', true);
                        
                         $('#iconodatos').show();
               }else{
                   $('#iconodatos').hide();
               }                    
                },
                error: function (error) {
                    console.log(error);
                }
            });
        }
    });
</script>
