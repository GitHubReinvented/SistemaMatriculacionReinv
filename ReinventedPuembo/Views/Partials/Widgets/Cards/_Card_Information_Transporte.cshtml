                                <a class="btn btn-secondary d-flex align-items-center opacity-75-hover fs-6 fw-semibold" style="font-size: 10px;" data-bs-toggle="modal" data-bs-target="#ModalSeguro">
                                    Mas información
                                </a>
                            }

                        }
                        <!--end::Actions-->
                    </div>
                    <!--end::Footer-->
