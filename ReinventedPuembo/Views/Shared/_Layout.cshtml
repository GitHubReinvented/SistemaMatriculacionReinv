<!DOCTYPE html>
<html @Html.Raw(KTTheme.PrintHtmlAttributes("html"))>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ReinventedPuembo.styles.css" asp-append-version="true" />

    <link rel="shortcut icon" href="@KTTheme.GetFavicon()" />
    <!--begin::Fonts-->
    @foreach (string font in KTTheme.GetFonts())
    {
        <link rel="stylesheet" href="@font" />
    }
    <!--end::Fonts-->
    <!--begin::Vendor Stylesheets(optional)-->
    @foreach (string file in KTTheme.GetVendors("Css"))
    {
        <link rel="stylesheet" type="text/css" href="@file" />
    }
    <!--end::Vendor Stylesheets-->
    <!--begin::Global Stylesheets(mandatory)-->
    @foreach (string file in KTTheme.GetGlobalAssets("Css"))
    {
        <link rel="stylesheet" type="text/css" href="@file" />
    }
    <!--end::Global Stylesheets-->
    <!--begin::Custom Stylesheets(optional)-->
    @foreach (string file in KTTheme.GetCssFiles())
    {
        <link rel="stylesheet" type="text/css" href='@KTTheme.GetAssetPath(file)' />
    }
    <!--end::Custom Stylesheets-->
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">ReinventedPuembo</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        @*                        <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>*@

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="CerrarSesion">Cerrar Sesión</a>
                        </li>
                    </ul>
                    @*<partial name="_LoginPartial" />*@
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios@1.1.2/dist/axios.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)

    <!--begin::Global Javascript(mandatory)-->
    @foreach (string file in KTTheme.GetGlobalAssets("Js"))
    {
        <script src="@file"></script>
    }
    <!--end::Global Javascript-->
    <!--begin::Vendors Javascript(optional)-->
    @foreach (string file in KTTheme.GetVendors("Js"))
    {
        <script src="@file"></script>
    }
    <!--end::Vendors Javascript-->
    <!--begin::Custom Javascript(optional)-->
    @foreach (string file in KTTheme.GetJavascriptFiles())
    {
        <script src='@KTTheme.GetAssetPath(file)'></script>
    }
    <!--end::Custom Javascript-->
</body>
</html>
